{
    "config": {
        "critical_width": 1920,
        "critical_height": 1080
    },
    "scripts": {
        "css:sass": "node-sass --output-style compressed ./_scss/style.scss ./_public/bundle.css",
        "css:sass:dev": "node-sass --source-map true ./_scss/style.scss ./_public/bundle.css",
        "css:postcss:autoprefixer": "postcss --use autoprefixer --output ./_public/bundle.css ./_public/bundle.css",
        "css:postcss:tailwind": "postcss --use tailwindcss cssnano --output ./_public/bundle.css ./_public/bundle.css",
        "css:postcss:config": "postcss --output ./_public/bundle.css ./_public/bundle.css",
        "css:critical": "from-env cross-var critical %DOMAIN --width $npm_package_config_critical_width --height $npm_package_config_critical_height  --ignore \"@font-face\" --ignore \".foo\" --userAgent \"Mozilla/5.0 (compatible; Googlebot/2.1; +http://www.google.com/bot.html)\" --timeout 30000 > ./_public/bundle-critical.css",
        "css:libs": "find ./_libs -name \"*.css\" | xargs cat > ./_public/libs.css && cat ./_public/bundle.css >> ./_public/libs.css && mv ./_public/libs.css ./_public/bundle.css",
        "css": "npm-run-all --sequential css:sass css:postcss:tailwind css:postcss:autoprefixer css:critical css:libs",
        "css:dev": "npm-run-all --sequential css:sass:dev css:postcss:tailwind css:libs",
        "js:browserify": "browserify ./_js/script.js --outfile ./_public/bundle.js --debug --transform babelify --transform vueify",
        "js:minify": "terser --compress --mangle --output ./_public/bundle.js ./_public/bundle.js",
        "js:babel": "npx babel ./_js/ --out-dir ./_build/_js/",
        "js:libs": "find ./_libs -name \"*.js\" | xargs cat | terser --output ./_public/libs.js && cat ./_public/bundle.js >> ./_public/libs.js && mv ./_public/libs.js ./_public/bundle.js",
        "js:tests": "jest --verbose --noStackTrace --no-cache || true",
        "js": "npm-run-all --sequential js:browserify js:minify js:babel js:libs js:tests",
        "js:dev": "npm-run-all --sequential js:browserify js:babel js:libs js:tests",
        "html": "html-minifier --collapse-whitespace --output ./_public/index.html ./_html/index.html",
        "html:dev": "html-minifier --output ./_public/index.html ./_html/index.html",
        "browsersync": "from-env browser-sync start --reload-debounce 3000 --proxy %DOMAIN --files \"./_public/*.css, ./_public/*.js\"",
        "browsersync:native": "browser-sync start --reload-debounce 3000 --proxy $(grep DOMAIN .env | cut -d '=' -f2) --files \"./_public/*.css, ./_public/*.js\"",
        "copy": "ncp ./_assets ./_public/",
        "delete": "del-cli ./_public/index.html",
        "replace": "replace-in-file \"/Copyright/g\" \"Hurray\" ./_public/index.html --isRegex",
        "watch:copy": "onchange ./_assets/*.* ./_assets/**/*.* --initial --wait --delay 1000 --await-write-finish -- onerror \"npm run copy\" --title \"watch:copy\" --message \"build failed\" --sound mute",
        "watch:css": "onchange ./_scss/*.scss ./_scss/**/*.scss ./_libs/*.css ./_libs/**/*.css --initial --wait --delay 1000 --await-write-finish -- onerror \"npm run css:dev\" --title \"watch:css\" --message \"build failed\" --sound mute",
        "watch:js": "onchange ./_js/*.js ./_js/**/*.js ./_vue/*.vue ./_vue/**/*.vue ./_libs/*.js ./_libs/**/*.js --initial --wait --delay 1000 --await-write-finish -- onerror \"npm run js:dev\" --title \"watch:js\" --message \"build failed\" --sound mute",
        "watch:js-tests": "onchange ./_tests/_js/*.js ./_tests/_js/**/*.js --wait --delay 1000 --await-write-finish -- onerror \"npm run js:tests\" --title \"watch:js-tests\" --message \"build failed\" --sound mute",
        "watch:html": "onchange ./_html/*.html ./_html/**/*.html --initial --wait --delay 1000 --await-write-finish -- onerror \"npm run html:dev\" --title \"watch:html\" --message \"build failed\" --sound mute",
        "prettier": "onchange ./_scss/*.scss ./_scss/**/*.scss ./_libs/*.css ./_libs/**/*.css ./_js/*.js ./_js/**/*.js ./_tests/_js/*.js ./_tests/_js/**/*.js ./_html/*.html ./_html/**/*.html ./_php/*.php ./_php/**/*.php -- prettier --write {{changed}}",
        "build": "npm-run-all --parallel copy css js html",
        "dev": "npm-run-all --parallel watch:* browsersync"
    },
    "devDependencies": {
        "@babel/cli": "^7.6.4",
        "@babel/core": "^7.6.4",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
        "@babel/plugin-proposal-optional-chaining": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.6.2",
        "@babel/polyfill": "^7.4.4",
        "@babel/preset-env": "^7.6.3",
        "@babel/runtime": "^7.6.3",
        "@prettier/plugin-php": "^0.11.2",
        "autoprefixer": "^9.6.4",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^24.9.0",
        "babel-plugin-array-includes": "^2.0.3",
        "babelify": "^10.0.0",
        "browser-sync": "^2.26.7",
        "browserify": "^16.5.0",
        "cli-error-notifier": "^2.1.0",
        "concat": "^1.0.3",
        "cpx": "^1.5.0",
        "critical": "^1.3.6",
        "cross-spawn": "^7.0.1",
        "cross-var": "^1.1.0",
        "cssnano": "^4.1.10",
        "del-cli": "^3.0.0",
        "dotenv": "^8.1.0",
        "element-closest": "^3.0.1",
        "env-cmd": "^10.0.1",
        "eslint": "^6.5.1",
        "exit": "^0.1.2",
        "from-env": "^1.1.4",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-babel": "^8.0.0",
        "gulp-clean-css": "^4.2.0",
        "gulp-concat": "^2.6.1",
        "gulp-connect": "^5.6.1",
        "gulp-htmlmin": "^5.0.1",
        "gulp-jest": "^4.0.3",
        "gulp-penthouse": "^0.2.0",
        "gulp-rename": "^1.4.0",
        "gulp-sass": "^4.0.1",
        "gulp-sourcemaps": "^2.6.5",
        "gulp-uglify": "^3.0.1",
        "gulp-vueify": "0.0.3",
        "hlp": "^2.4.6",
        "html-minifier": "^4.0.0",
        "ismobilejs": "^1.0.3",
        "jest": "^24.9.0",
        "jest-cli": "^24.9.0",
        "jest-image-snapshot": "^2.10.0",
        "jest-puppeteer": "^4.3.0",
        "mdn-polyfills": "^5.18.0",
        "ncp": "^2.0.0",
        "node-sass": "^4.12.0",
        "npm-check-updates": "^3.1.24",
        "npm-run-all": "^4.1.5",
        "onchange": "^6.1.0",
        "postcss-cli": "^6.1.3",
        "postcss-url": "^8.0.0",
        "prettier": "^1.18.2",
        "puppeteer": "^1.19.0",
        "regenerator-runtime": "^0.13.3",
        "replace-in-file": "^4.1.3",
        "run-sequence": "^2.2.1",
        "tailwindcss": "^1.1.2",
        "terser": "^4.3.8",
        "through-gulp": "^0.5.0",
        "uglify": "^0.1.5",
        "uglify-js": "^3.6.1",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0",
        "vue": "^2.6.7",
        "vue-template-compiler": "^2.6.7",
        "vueify": "^9.4.1",
        "vueify-insert-css": "^1.0.0",
        "whatwg-fetch": "^3.0.0"
    },
    "name": "boilerplate",
    "version": "1.0.0",
    "description": "",
    "main": "_public/bundle.js",
    "author": "",
