{
    "scripts": {
        "🔽 css 🔽": "",
        "css": "npm-run-all --sequential css:sass css:postcss:tailwind css:postcss:autoprefixer css:critical css:libs",
        "css:dev": "npm-run-all --sequential css:sass:dev css:postcss:tailwind css:libs browsersync:reload",
        "css:watch": "onchange ./_scss/*.scss ./_scss/**/*.scss ./*.php ./**/*.php ./_php/*.php ./_php/**/*.php ./_libs/*.css ./_libs/**/*.css --initial --poll 25 --delay 0 --await-write-finish 25 -- onerror \"npm run css:dev\" --title \"css:watch\" --message \"build failed\" --sound mute",
        "css:sass": "node-sass --output-style compressed ./_scss/style.scss ./_public/bundle.css",
        "css:sass:dev": "node-sass --source-map true ./_scss/style.scss ./_public/bundle.css",
        "css:postcss:autoprefixer": "postcss --use autoprefixer --no-map --output ./_public/bundle.css ./_public/bundle.css",
        "css:postcss:tailwind": "cross-env TAILWIND_MODE=build postcss --use tailwindcss cssnano --no-map --no-autoprefixer --output ./_public/bundle.css ./_public/bundle.css",
        "css:postcss:config": "postcss --no-map --output ./_public/bundle.css ./_public/bundle.css",
        "css:critical": "from-env cross-var critical %DOMAIN --width $npm_package_config_critical_width --height $npm_package_config_critical_height --ignore-atrule \"@font-face\" --ignore-rule \".foo\" --userAgent \"Mozilla/5.0 (compatible; Googlebot/2.1; +http://www.google.com/bot.html)\" > ./_public/bundle-critical.css",
        "css:libs": "find ./_libs -name \"*.css\" | xargs cat > ./_public/libs.css && cat ./_public/bundle.css >> ./_public/libs.css && mv ./_public/libs.css ./_public/bundle.css",
        "                                                                                                                                                                                                      ": "",
        "🔽 js 🔽": "",
        "js": "npm-run-all --sequential js:browserify js:minify js:babel js:libs js:tests",
        "js:dev": "npm-run-all --sequential js:browserify js:babel js:libs js:tests browsersync:reload",
        "js:watch": "onchange ./_js/*.js ./_js/**/*.js ./_vue/*.vue ./_vue/**/*.vue ./_libs/*.js ./_libs/**/*.js --initial --poll 25 --delay 0 --await-write-finish 25 -- onerror \"npm run js:dev\" --title \"js:watch\" --message \"build failed\" --sound mute",
        "js:browserify": "browserify ./_js/script.js --outfile ./_public/bundle.js --debug -g [ browserify-css --minify=true --stripComments=true ] --transform babelify --transform vueify",
        "#️⃣ if you want to transform dependencies (which are e.g. outside of the root directory)": "",
        "js:browserify:with-transform-dependencies": "browserify ./_js/script.js --outfile ./_public/bundle.js --debug -g [ browserify-css --minify=true --stripComments=true ] -g [ babelify --presets [ @babel/preset-env ] ] --transform vueify",
        "js:minify": "terser --compress --mangle --output ./_public/bundle.js ./_public/bundle.js",
        "js:babel": "npx babel ./_js/ --out-dir ./_build/_js/",
        "js:libs": "find ./_libs -name \"*.js\" | xargs cat | terser --output ./_public/libs.js && cat ./_public/bundle.js >> ./_public/libs.js && mv ./_public/libs.js ./_public/bundle.js",
        "js:tests": "jest --verbose --noStackTrace --no-cache || true",
        "js:tests:watch": "onchange ./_tests/_js/*.js ./_tests/_js/**/*.js --poll 25 --delay 0 --await-write-finish 25 -- onerror \"npm run js:tests\" --title \"js:tests:watch\" --message \"build failed\" --sound mute",
        "                                                                                                                                                                                                       ": "",
        "🔽 html 🔽": "",
        "html": "npm-run-all --sequential html:minify html:inline",
        "html:dev": "npm-run-all --sequential html:minify:dev",
        "html:watch": "onchange ./_html/*.html ./_html/**/*.html --initial --poll 25 --delay 0 --await-write-finish 25 -- onerror \"npm run html:dev\" --title \"html:watch\" --message \"build failed\" --sound mute",
        "html:minify": "html-minifier --collapse-whitespace --remove-comments --remove-optional-tags --remove-redundant-attributes --remove-script-type-attributes --minify-css true --minify-js true --input-dir ./_html --output-dir ./_public --file-ext html",
        "html:minify:dev": "html-minifier --input-dir ./_html --output-dir ./_public --file-ext html",
        "html:inline": "inline-source --attribute \"data-inline\" --compress true --root ./_public ./_public/index.html > ./_public/index_inline.html && move-file ./_public/index_inline.html ./_public/index.html",
        "                                                                                                                                                                                                        ": "",
        "🔽 browsersync 🔽": "",
        "browsersync": "from-env browser-sync start --no-open --proxy %DOMAIN",
        "browsersync:reload": "browser-sync reload",
        "browsersync:watch": "from-env browser-sync start --no-open --reload-debounce 3000 --proxy %DOMAIN --files \"./_public/*.html, ./_public/*.css, ./_public/*.js\"",
        "browsersync:native": "browser-sync start --no-open --reload-debounce 3000 --proxy $(grep DOMAIN .env | cut -d '=' -f2) --files \"./_public/*.html, ./_public/*.css, ./_public/*.js\"",
        "                                                                                                                                                                                                         ": "",
        "🔽 utils 🔽": "",
        "concat": "concat -o output.css ./1.css ./2.css ./3.css",
        "copy": "ncp ./_assets ./_public/",
        "copy:watch": "onchange ./_assets/*.* ./_assets/**/*.* --initial --poll 25 --delay 0 --await-write-finish 25 -- onerror \"npm run copy\" --title \"copy:watch\" --message \"build failed\" --sound mute",
        "delete:file": "del-cli ./_public/index.html",
        "delete:files": "del-cli ./_public/*.* ./_public/**/ !./_public/index.html",
        "replace": "replace-in-file \"/Copyright/g\" \"Hurray\" ./_public/index.html --isRegex",
        "prettier": "onchange ./_scss/*.scss ./_scss/**/*.scss ./_libs/*.css ./_libs/**/*.css ./_js/*.js ./_js/**/*.js ./_tests/_js/*.js ./_tests/_js/**/*.js ./_html/*.html ./_html/**/*.html ./_php/*.php ./_php/**/*.php -- prettier --write {{changed}}",
        "                                                                                                                                                                                                          ": "",
        "🔽 pipelines 🔽": "",
        "prod": "cross-env NODE_ENV=production npm-run-all --sequential copy css js html delete:files",
        "dev": "cross-env NODE_ENV=development npm-run-all --parallel copy:watch css:watch js:watch js:tests:watch html:watch browsersync"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.5",
        "@babel/core": "^7.14.6",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/plugin-proposal-object-rest-spread": "^7.14.5",
        "@babel/plugin-proposal-optional-chaining": "^7.14.5",
        "@babel/plugin-proposal-private-methods": "^7.14.5",
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.14.5",
        "@babel/preset-react": "^7.14.5",
        "@babel/runtime": "^7.14.6",
        "@prettier/plugin-php": "^0.16.3",
        "autoprefixer": "^10.2.5",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^26.6.3",
        "babel-plugin-array-includes": "^2.0.3",
        "babelify": "^10.0.0",
        "browser-sync": "^2.26.14",
        "browserify": "^17.0.0",
        "browserify-css": "^0.15.0",
        "cli-error-notifier": "^2.1.0",
        "concat": "^1.0.3",
        "core-js": "^3.10.0",
        "cpx": "^1.5.0",
        "critical": "^2.0.6",
        "cross-env": "^7.0.3",
        "cross-spawn": "^7.0.3",
        "cross-var": "^1.1.0",
        "cssnano": "^4.1.11",
        "del-cli": "^3.0.1",
        "dotenv": "^8.2.0",
        "element-closest": "^3.0.2",
        "env-cmd": "^10.1.0",
        "eslint": "^7.29.0",
        "exit": "^0.1.2",
        "from-env": "^1.1.4",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-babel": "^8.0.0",
        "gulp-clean-css": "^4.3.0",
        "gulp-concat": "^2.6.1",
        "gulp-connect": "^5.7.0",
        "gulp-htmlmin": "^5.0.1",
        "gulp-jest": "^4.0.3",
        "gulp-penthouse": "^0.2.0",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^4.1.0",
        "gulp-sourcemaps": "^3.0.0",
        "gulp-uglify": "^3.0.2",
        "gulp-vueify": "0.0.3",
        "highlight.js": "^10.7.2",
        "hlp": "^2.8.0",
        "html-minifier": "^4.0.0",
        "inline-source-cli": "^2.0.0",
        "ismobilejs": "^1.1.1",
        "jest": "^26.6.3",
        "jest-cli": "^26.6.3",
        "jest-image-snapshot": "^4.4.1",
        "jest-puppeteer": "^4.4.0",
        "mdn-polyfills": "^5.20.0",
        "move-file-cli": "^2.0.0",
        "ncp": "^2.0.0",
        "node-sass": "^5.0.0",
        "npm-check-updates": "^10.3.1",
        "npm-run-all": "^4.1.5",
        "onchange": "^7.1.0",
        "postcss": "^8.3.5",
        "postcss-cli": "^8.3.1",
        "postcss-tailwind-data-attr": "^1.0.6",
