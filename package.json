        "through-gulp": "^0.5.0",
        "uglify": "^0.1.5",
        "uglify-js": "^3.13.3",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0",
        "vue": "^2.6.12",
