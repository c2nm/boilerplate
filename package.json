{
  "scripts": {
    "css:sass": "node-sass --output-style compressed ./_scss/style.scss ./_build/bundle.css",
    "css:autoprefix": "postcss --use autoprefixer --output ./_build/bundle.css ./_build/bundle.css",
    "css:critical": "critical https://vielhuber.de --width 1920 --height 1080 --userAgent \"Mozilla/5.0 (compatible; Googlebot/2.1; +http://www.google.com/bot.html)\" --timeout 30000 > ./_build/bundle-critical.css",
    "css": "npm-run-all --sequential css:sass css:autoprefix css:critical",
    "js:browserify": "browserify ./_js/script.js --outfile ./_build/bundle.js --debug --transform babelify",
    "js:uglify": "uglifyjs --compress --mangle --output ./_build/bundle.js ./_build/bundle.js",
    "js:babel": "npx babel ./_js/ --out-dir ./_build/_js/ --plugins=transform-runtime --presets=es2015,es2017",
    "js:tests": "jest",
    "js": "npm-run-all --sequential js:browserify js:uglify js:babel js:tests",
    "html": "html-minifier --collapse-whitespace --output ./index.html ./_html/index.html",
    "browsersync": "browser-sync start --proxy \"https://vielhuber.de\" --files \"./_build/*.css, ./_build/*.js\"",
    "copy": "cpx \"./_assets/**/*.{html,png,jpg}\" ./_build/",
    "watch:copy": "watch \"npm run copy\" ./_assets/ --interval=0.1",
    "watch:css": "watch \"npm run css\" ./_scss/ --interval=0.1",
    "watch:js": "watch \"npm run js\" ./_js/ --interval=0.1",
    "watch:js-tests": "watch \"npm run js:tests\" ./_tests/_js/ --interval=0.1",
    "watch:html": "watch \"npm run html\" ./_html/ --interval=0.1",
    "build": "npm-run-all --parallel watch:*",
    "serve": "npm-run-all --parallel watch:* browsersync"
  },
  "dependencies": {
    "autoprefixer": "^9.0.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "babelify": "^8.0.0",
    "browser-sync": "^2.24.6",
    "browserify": "^16.2.2",
    "cpx": "^1.5.0",
    "critical": "^1.3.4",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-clean-css": "^3.9.4",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.5.0",
    "gulp-htmlmin": "^4.0.0",
    "gulp-jest": "^4.0.2",
    "gulp-penthouse": "^0.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.1",
    "gulp-vueify": "0.0.3",
    "hlp": "^2.1.5",
    "html-minifier": "^3.5.19",
    "ismobilejs": "^0.4.1",
    "jest": "^23.4.2",
    "jest-cli": "^23.4.2",
    "jest-puppeteer": "^3.2.1",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0",
    "puppeteer": "^1.6.1",
    "regenerator-runtime": "^0.12.0",
    "run-sequence": "^2.2.1",
    "through-gulp": "^0.5.0",
    "uglify": "^0.1.5",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "vue": "^2.5.17",
    "vueify-insert-css": "^1.0.0",
    "whatwg-fetch": "^2.0.4"
  },
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "_build/bundle.js",
  "devDependencies": {},
  "author": "",
