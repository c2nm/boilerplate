{
    "config": {
        "domain": "https://vielhuber.de"
    },
    "scripts": {
        "css:sass": "node-sass --source-map true --output-style compressed ./_scss/style.scss ./_build/bundle.css",
        "css:autoprefix": "postcss --use autoprefixer --output ./_build/bundle.css ./_build/bundle.css",
        "css:critical": "cross-var critical $npm_package_config_domain --width 1920 --height 1080 --userAgent \"Mozilla/5.0 (compatible; Googlebot/2.1; +http://www.google.com/bot.html)\" --timeout 30000 > ./_build/bundle-critical.css",
        "css:libs": "find ./_libs -name \"*.css\" | xargs cat > ./_build/libs.css && cat ./_build/bundle.css >> ./_build/libs.css && mv ./_build/libs.css ./_build/bundle.css",
        "css": "npm-run-all --sequential css:sass css:autoprefix css:critical css:libs",
        "js:browserify": "browserify ./_js/script.js --outfile ./_build/bundle.js --debug --transform babelify --transform vueify",
        "js:uglify": "uglifyjs --compress --mangle --output ./_build/bundle.js ./_build/bundle.js",
        "js:babel": "npx babel ./_js/ --out-dir ./_build/_js/ --plugins=transform-runtime,transform-class-properties,transform-object-rest-spread --presets=es2015,es2017",
        "js:libs": "find ./_libs -name \"*.js\" | xargs cat | uglifyjs --output ./_build/libs.js && cat ./_build/bundle.js >> ./_build/libs.js && mv ./_build/libs.js ./_build/bundle.js",
        "js:tests": "jest --verbose --noStackTrace --no-cache || echo",
        "js": "npm-run-all --sequential js:browserify js:uglify js:babel js:libs js:tests",
        "html": "html-minifier --collapse-whitespace --output ./index.html ./_html/index.html",
        "browsersync": "cross-var browser-sync start --reload-debounce 3000 --proxy \"$npm_package_config_domain\" --files \"./_build/*.css, ./_build/*.js\"",
        "copy": "cpx \"./_assets/**/*.{html,png,jpg}\" ./_build/",
        "watch:copy": "onchange ./_assets/**/*.* --initial --wait --delay 1000 --await-write-finish -- \"npm run copy\"",
        "watch:css": "onchange ./_scss/**/*.scss ./_libs/**/*.css --initial --wait --delay 1000 --await-write-finish -- \"npm run css\"",
        "watch:js": "onchange ./_js/**/*.js ./_libs/**/*.js --initial --wait --delay 1000 --await-write-finish -- \"npm run js\"",
        "watch:js-tests": "onchange ./_tests/_js/**/*.js --wait --delay 1000 --await-write-finish -- \"npm run js:tests\"",
        "watch:html": "onchange ./_html/**/*.html --initial --wait --delay 1000 --await-write-finish -- \"npm run html\"",
        "prettier": "onchange ./_scss/**/*.scss ./_js/**/*.js ./_tests/_js/**/*.js ./_html/**/*.html ./_php/**/*.php -- prettier --write {{changed}}",
        "build": "npm-run-all --parallel watch:*",
        "serve": "npm-run-all --parallel watch:* browsersync"
    },
    "dependencies": {
        "@prettier/plugin-php": "^0.6.0",
        "autoprefixer": "^9.1.5",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-jest": "^23.4.2",
        "babel-plugin-array-includes": "^2.0.3",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-es2017": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "babelify": "^8.0.0",
        "browser-sync": "^2.24.7",
        "browserify": "^16.2.2",
        "concat": "^1.0.3",
        "cpx": "^1.5.0",
        "critical": "^1.3.4",
        "cross-var": "^1.1.0",
        "eslint": "^5.5.0",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "^6.0.0",
        "gulp-babel": "^8.0.0",
        "gulp-clean-css": "^3.10.0",
        "gulp-concat": "^2.6.1",
        "gulp-connect": "^5.6.1",
        "gulp-htmlmin": "^4.0.0",
        "gulp-jest": "^4.0.2",
        "gulp-penthouse": "^0.2.0",
        "gulp-rename": "^1.4.0",
        "gulp-sass": "^4.0.1",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-uglify": "^3.0.1",
        "gulp-vueify": "0.0.3",
        "hlp": "^2.2.0",
        "html-minifier": "^3.5.20",
        "ismobilejs": "^0.4.1",
        "jest": "^23.5.0",
        "jest-cli": "^23.5.0",
        "jest-image-snapshot": "^2.5.0",
        "jest-puppeteer": "^3.3.1",
        "node-sass": "^4.9.3",
        "npm-run-all": "^4.1.3",
        "onchange": "^4.1.0",
        "postcss-cli": "^6.0.0",
        "prettier": "^1.14.2",
        "puppeteer": "^1.8.0",
        "regenerator-runtime": "^0.12.1",
        "run-sequence": "^2.2.1",
        "through-gulp": "^0.5.0",
        "uglify": "^0.1.5",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0",
        "vue": "^2.5.17",
        "vueify": "^9.4.1",
        "vueify-insert-css": "^1.0.0",
        "whatwg-fetch": "^2.0.4"
    },
    "name": "boilerplate",
    "version": "1.0.0",
    "description": "",
    "main": "_build/bundle.js",
    "author": "",
    "license": "ISC"
}
