{
    "config": {
        "critical_width": 1920,
        "critical_height": 1080
    },
    "scripts": {
        "css:sass": "node-sass --output-style compressed ./_scss/style.scss ./_public/bundle.css",
        "css:sass:dev": "node-sass --source-map true ./_scss/style.scss ./_public/bundle.css",
        "css:postcss": "postcss --output ./_public/bundle.css ./_public/bundle.css",
        "css:critical": "from-env cross-var critical %DOMAIN --width $npm_package_config_critical_width --height $npm_package_config_critical_height  --ignore \"@font-face\" --ignore \".foo\" --userAgent \"Mozilla/5.0 (compatible; Googlebot/2.1; +http://www.google.com/bot.html)\" --timeout 30000 > ./_public/bundle-critical.css",
        "css:libs": "find ./_libs -name \"*.css\" | xargs cat > ./_public/libs.css && cat ./_public/bundle.css >> ./_public/libs.css && mv ./_public/libs.css ./_public/bundle.css",
        "css": "npm-run-all --sequential css:sass css:postcss css:critical css:libs",
        "css:dev": "npm-run-all --sequential css:sass:dev css:libs",
        "js:browserify": "browserify ./_js/script.js --outfile ./_public/bundle.js --debug --transform babelify --transform vueify",
        "js:uglify": "uglifyjs --compress --mangle --output ./_public/bundle.js ./_public/bundle.js",
        "js:babel": "npx babel ./_js/ --out-dir ./_build/_js/",
        "js:libs": "find ./_libs -name \"*.js\" | xargs cat | uglifyjs --output ./_public/libs.js && cat ./_public/bundle.js >> ./_public/libs.js && mv ./_public/libs.js ./_public/bundle.js",
        "js:tests": "jest --verbose --noStackTrace --no-cache || true",
        "js": "npm-run-all --sequential js:browserify js:uglify js:babel js:libs js:tests",
        "js:dev": "npm-run-all --sequential js:browserify js:babel js:libs js:tests",
        "html": "html-minifier --collapse-whitespace --output ./_public/index.html ./_html/index.html",
        "html:dev": "html-minifier --output ./_public/index.html ./_html/index.html",
        "browsersync": "from-env cross-var browser-sync start --reload-debounce 3000 --proxy %DOMAIN --files \"./_public/*.css, ./_public/*.js\"",
        "copy": "ncp ./_assets ./_public/",
        "delete": "del-cli ./_public/index.html",
        "replace": "replace-in-file \"/Copyright/g\" \"Hurray\" ./_public/index.html --isRegex",
        "watch:copy": "onchange ./_assets/**/*.* --initial --wait --delay 1000 --await-write-finish -- onerror \"npm run copy --silent\" --title \"watch:copy\" --message \"build failed\" --sound mute",
        "watch:css": "onchange ./_scss/**/*.scss ./_libs/**/*.css --initial --wait --delay 1000 --await-write-finish -- onerror \"npm run css:dev --silent\" --title \"watch:css\" --message \"build failed\" --sound mute",
        "watch:js": "onchange ./_js/**/*.js ./_vue/**/*.vue ./_libs/**/*.js --initial --wait --delay 1000 --await-write-finish -- onerror \"npm run js:dev --silent\" --title \"watch:js\" --message \"build failed\" --sound mute",
        "watch:js-tests": "onchange ./_tests/_js/**/*.js --wait --delay 1000 --await-write-finish -- onerror \"npm run js:tests --silent\" --title \"watch:js-tests\" --message \"build failed\" --sound mute",
        "watch:html": "onchange ./_html/**/*.html --initial --wait --delay 1000 --await-write-finish -- onerror \"npm run html:dev --silent\" --title \"watch:html\" --message \"build failed\" --sound mute",
        "prettier": "onchange ./_scss/**/*.scss ./_js/**/*.js ./_tests/_js/**/*.js ./_html/**/*.html ./_php/**/*.php -- prettier --write {{changed}}",
        "build": "npm-run-all --parallel copy css js html",
        "dev": "npm-run-all --parallel watch:* browsersync"
    },
    "dependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.2.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
        "@babel/plugin-proposal-optional-chaining": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.2.0",
        "@babel/polyfill": "^7.2.5",
        "@babel/preset-env": "^7.2.3",
        "@babel/runtime": "^7.2.0",
        "@prettier/plugin-php": "^0.9.0",
        "autoprefixer": "^9.4.5",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^23.6.0",
        "babel-plugin-array-includes": "^2.0.3",
        "babelify": "^10.0.0",
        "browser-sync": "^2.26.3",
        "browserify": "^16.2.3",
        "cli-error-notifier": "^2.0.1",
        "concat": "^1.0.3",
        "cpx": "^1.5.0",
        "critical": "^1.3.4",
        "cross-spawn": "^6.0.5",
        "cross-var": "^1.1.0",
        "del-cli": "^1.1.0",
        "dotenv": "^6.2.0",
        "element-closest": "^2.0.2",
        "env-cmd": "^8.0.2",
        "eslint": "^5.12.0",
        "exit": "^0.1.2",
        "from-env": "^1.1.2",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "^6.0.0",
        "gulp-babel": "^8.0.0",
        "gulp-clean-css": "^3.10.0",
        "gulp-concat": "^2.6.1",
        "gulp-connect": "^5.6.1",
        "gulp-htmlmin": "^5.0.1",
        "gulp-jest": "^4.0.2",
        "gulp-penthouse": "^0.2.0",
        "gulp-rename": "^1.4.0",
        "gulp-sass": "^4.0.1",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-uglify": "^3.0.1",
        "gulp-vueify": "0.0.3",
        "hlp": "^2.3.4",
        "html-minifier": "^3.5.21",
        "ismobilejs": "^0.5.1",
        "jest": "^23.6.0",
        "jest-cli": "^23.6.0",
        "jest-image-snapshot": "^2.7.0",
        "jest-puppeteer": "^3.8.0",
        "ncp": "^2.0.0",
        "node-sass": "^4.11.0",
        "npm-run-all": "^4.1.5",
        "onchange": "^5.2.0",
        "postcss-cli": "^6.1.1",
        "postcss-url": "^8.0.0",
        "prettier": "^1.15.3",
        "puppeteer": "^1.11.0",
        "regenerator-runtime": "^0.13.1",
        "run-sequence": "^2.2.1",
        "through-gulp": "^0.5.0",
        "uglify": "^0.1.5",
        "vinyl-buffer": "^1.0.1",
        "vinyl-source-stream": "^2.0.0",
        "vue": "^2.5.22",
        "vue-template-compiler": "^2.5.22",
        "vueify": "^9.4.1",
        "vueify-insert-css": "^1.0.0",
        "whatwg-fetch": "^3.0.0"
    },
    "name": "boilerplate",
    "version": "1.0.0",
    "description": "",
    "main": "_public/bundle.js",
    "author": "",
    "license": "ISC"
}
